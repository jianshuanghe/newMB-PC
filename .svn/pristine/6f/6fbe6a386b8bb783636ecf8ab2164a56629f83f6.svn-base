    $(".Releaserecomm-Choice-son3").click(function(){
        $(".Releaserecomm-Choice-son3").css({"color":" #02C2A2",'font-weight': 'bold'});
        $(".span3").css('display','block')
        $(".Releaserecomm-Choice-son2").css({"color":" #2E2E30",'font-weight': 'normal'});
