        $(".span1").css('display','none')
        $(".Releaserecomm-Choice-son4").css({"color":" #2E2E30",'font-weight': 'normal'});
        $(".span4").css('display','none')
    })
    $(".Releaserecomm-Choice-son4").click(function(){
        $(".Releaserecomm-Choice-son4").css({"color":" #02C2A2",'font-weight': 'bold'});
        $(".span4").css('display','block')
        $(".Releaserecomm-Choice-son2").css({"color":" #2E2E30",'font-weight': 'normal'});
        $(".span2").css('display','none')
        $(".Releaserecomm-Choice-son3").css({"color":" #2E2E30",'font-weight': 'normal'});
        $(".span3").css('display','none')
        $(".Releaserecomm-Choice-son1").css({"color":" #2E2E30",'font-weight': 'normal'});
        $(".span1").css('display','none')
    })
</script>