<script>
    $(function () {
        /*公共部分
         * header 头部
         * header 底部
         */
        $("#header").load("./../../../src/common/pages/header/header.html");
        $(".input").focus();
    });
    $(".zonghe").click(function(){
        $(".zonghe").css('color','#02C2A2');
        $(".renqi").css('color','#2E2E30');
    });
    $(".renqi").click(function(){
        $(".zonghe").css('color','#2E2E30');
        $(".renqi").css('color','#02C2A2');
    });
    let num=1;
    $(".Authentication").click(function(){
        if(num==1){
            $(".image").attr('src','../../../images/img/in.png');
            num=2;
        }else{
            $(".image").attr('src','../../../images/img/not.png');
            num=1;
        }
    });
    $(".region").hover(function(){
        $(".imgdh").addClass('DH');
        $(".Province").css('display','block');
    },function(){
        $(".Province").css('display','none');
        $(".imgdh").removeClass('DH');
    })

    $(".fbtime").hover(function(){
        $(".fbtime-ti").css('display','block');
    },function(){
        $(".fbtime-ti").css('display','none');
    })
    $(".zhengxu").hover(function(){
        $(".zhengxu").css('color','#02C2A2');
    },function(){
        $(".zhengxu").css('color','#2E2E30');
    })
    $(".daoxu").hover(function(){
        $(".daoxu").css('color','#02C2A2');
    },function(){
        $(".daoxu").css('color','#2E2E30');
    })

    $(".search-gong").click(function(){
        $(".search-supply").css('display','block');
        $(".search-Purchase").css('display','none');
        $(".search-gong").css('color','#02C2A2');
        $(".search-cai").css('color','#2E2E30');
        $(".search-zi").css('color','#2E2E30');

    });
    $(".search-cai").click(function(){
        $(".search-supply").css('display','none');
        $(".search-Purchase").css('display','block');
        $(".search-gong").css('color','#2E2E30');
        $(".search-cai").css('color','#02C2A2');
        $(".search-zi").css('color','#2E2E30');
    });
    $(".search-zi").click(function(){
        $(".search-supply").css('display','none');
        $(".search-Purchase").css('display','none');
        $(".search-information").css('display','block');
        $(".search-gong").css('color','#2E2E30');
        $(".search-cai").css('color','#2E2E30');
        $(".search-zi").css('color','#02C2A2');
    });

    $(".Purchase").click(function(){
        $(".Purchase").css({"color":" #02C2A2",'font-weight': 'bold'});
        $(".Purchase-son").css('display','block');
        $(".supply-son").css('display','none');
        $(".supply").css({"color":" #2E2E30",'font-weight': 'normal'});
        $(".List-bodytwo").css('display','block');
        $(".List-body").css('display','none');
    })
    $(".supply").click(function(){
        $(".Purchase").css({"color":" #2E2E30",'font-weight': 'normal'});
        $(".Purchase-son").css('display','none');
        $(".supply-son").css('display','block');
        $(".supply").css({"color":"#02C2A2 ",'font-weight': 'bold'});
        $(".List-bodytwo").css('display','none');
        $(".List-body").css('display','block');
    })
</script>