.homepage-header{
    width: 100%;
    height: 69px;
    border: 1px solid #EFEFEF;
    position: fixed;
    top: 34px;
    background: white;
    z-index: 10;
}
.homepage-hea{
    height: 69px;
    margin: 0 auto;
    /* background: red; */
    overflow: hidden;
    width: 1200px;
}
.homepage-hea-img{
    width: 62px;
    height: 36.3px;
    margin-top: 16px;
    margin-left: 20px;
    float: left
}
.homepage-hea-img>img{
    width: 100%;
    height: 100%;
}
.homepage-hea-inp{
    float: left;
    width: 664px;
    height: 38px;
    border: 1px solid #EFEFEF;
    margin-top: 16px;
    margin-left: 65px;
    position: relative;
    border-radius: 4px;
}
.homepage-hea-inp>input{
    width: 592px;
    height: 100%;
    font-size: 14px;
    color: #D2D2D2;
    padding-left: 10px;
   
}
.homepage-hea-inp>input::-webkit-input-placeholder {
    font-size: 14px;
    color: #D2D2D2;
}
.homepage-hea-sech{
    position: absolute;
    right: 0;
    top: 0;
    width: 72px;
    height: 100%;
    background: #02C2A2;

}
.homepage-hea-sech>img{
    margin-left: 7px;
    margin-top: 8px;
    width: 20.8px;
    height: 21px;
    display: block;
    float: left;
}
.homepage-hea-sech>span{
    font-size: 16px;
    color: #FFFFFF;
    display: block;
    float: left;
    margin-top: 7px;
    margin-left: 3px;
}
.homepage-hea-gong{
    width: 104px;
    height: 38px;
    background: #F9F9F9;
    border: 1px solid #EFEFEF;
    border-radius: 4px;
    float: left;
    margin-top: 16px;
    font-size: 14px;
    color: #2E2E30;
    text-align: center;
    line-height: 38px;
}
.gongying{
    margin-left: 40px;
}
.caigou{
    margin-left: 18px;
}
.jia{
    margin-left: 17px;
    font-size: 16px;
    font-weight: bold;
}
